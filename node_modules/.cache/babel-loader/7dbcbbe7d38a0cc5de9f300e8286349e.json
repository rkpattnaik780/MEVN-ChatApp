idebarOpened = false;\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.getters[\"user/getUserDetails\"];\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.sidebar-card {\n  position: relative;\n}\n\n.sidebar-card-btn-container {\n  position: absolute;\n  bottom: 1rem;\n  margin-left: auto;\n  margin-right: auto;\n  left: 0;\n  right: 0;\n  text-align: center;\n}\n\n@media only screen and (max-width: 500px) {\n  .message-list {\n    padding: 1px;\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}