text-xs-center sidebar-card-btn-container\">\n            <v-btn class=\"v-btn--small\" @click=\"closeSidebar\" color=\"info\">\n                <v-icon>mdi-window-close</v-icon>\n            </v-btn>\n          </div>\n        </v-card>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\nimport MessageList from \"@/components/MessageList\";\nimport MessageTextField from \"@/components/MessageTextField\";\n\nexport default {\n  name: \"Messages\",\n  components: {\n    MessageList,\n    MessageTextField\n  },\n  data() {\n    return {\n      active_users: [],\n      isSidebarOpened: true\n    };\n  },\n  mounted() {\n    this.join();\n    this.socket.on(\"users_fetched\", data => {\n      console.log(data);\n      this.active_users = data;\n    });\n  },\n  methods: {\n    join() {\n      this.socket.emit(\"user_joined\", this.currentUser);\n    },\n    openSidebar() {\n      this.isSidebarOpened = true;\n    },\n    closeSidebar() {\n      this.isSidebarOpened = false;\n    }\n  },\n  computed: {\n    currentUser() {\n      return this.$store.getters[\"user/getUserDetails\"];\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.sidebar-card {\n  position: relative;\n}\n\n.sidebar-card-btn-container {\n  position: absolute;\n  bottom: 1rem;\n  margin-left: auto;\n  margin-right: auto;\n  left: 0;\n  right: 0;\n  text-align: center;\n}\n\n@media only screen and (max-width: 500px) {\n  .message-list {\n    padding: 1px;\n  }\n}\n</style>\n"]}]}