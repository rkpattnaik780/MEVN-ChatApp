  </v-toolbar>\n  </span>\n</template>\n\n<script>\n\nimport api from \"@/api\";\nexport default {\n  name: \"AppNavigation\",\n  computed :{\n    currentUser(){\n      return this.$store.getters[\"user/getUserDetails\"];\n    }\n  },\n  methods: {\n    signOut(){\n      this.disconnect();\n      api.signOut().then(res => {\n        this.$store.commit(\"user/reset\");\n        this.$router.push(\"/\");\n      });\n    },\n    disconnect() {\n      this.socket.emit(\"user_disconnected\", this.currentUser);\n    }\n  },\n  data() {\n    return {\n      drawer: false\n    };\n  }\n};\n</script>\n\n<style scoped></style>\n"]}]}